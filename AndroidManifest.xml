<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.iwaygo.Subway" android:versionCode="1"
	android:versionName="1.0">
	<uses-sdk android:targetSdkVersion="8" android:minSdkVersion="7"
		android:maxSdkVersion="11"></uses-sdk>


	<application android:label="@string/app_name" android:icon="@drawable/logo72_72">
		<activity android:name=".iWayGoSubway" android:label="@string/app_name"
			android:configChanges="keyboardHidden|orientation"
			android:screenOrientation="portrait" android:windowSoftInputMode="adjustResize">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".viewMap" android:screenOrientation="portrait"></activity>
		<activity android:name=".showWay" android:screenOrientation="portrait"></activity>
		<activity android:name=".stationInfo"
			android:screenOrientation="portrait"></activity>
		<activity android:name=".stationInfoline"
			android:screenOrientation="portrait"></activity>
		<activity android:name=".lineAndStation"
			android:screenOrientation="portrait"></activity>
		<activity android:name=".setting"
			android:screenOrientation="portrait"></activity>



	</application>
</manifest>