<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">
	<RelativeLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<ImageView android:paddingTop="0dip"
			android:layout_alignParentLeft="true" android:layout_width="wrap_content"
			android:layout_height="57dip" android:id="@+id/ImageManager"
			android:padding="10dip" />
		<TextView android:layout_height="57dip" android:textSize="20dip"
			android:layout_width="fill_parent" android:id="@+id/ItemManager"
			android:gravity="left" android:textColor="#000000" android:padding="10dip"
			android:layout_marginLeft="110dip" android:layout_marginTop="5dip"
			android:layout_toRightOf="@+id/ItemManager" />
		<ImageView android:layout_width="wrap_content" android:layout_centerVertical="true"
			android:layout_height="25dip" android:id="@+id/ImageManager2" android:paddingRight="10dip"
			android:src="@drawable/inner" android:layout_alignParentRight="true" />
	</RelativeLayout>
</LinearLayout>